{{#each this}}
<div class="photo-card">
    <img src="{{webformatURL}}" alt="" />

    <div class="stats">
        <p class="stats-item">
            <i class="material-icons">thumb_up {{favorites}}</i>

        </p>
        <p class="stats-item">
            <i class="material-icons">visibility {{views}}</i>

        </p>
        <p class="stats-item">
            <i class="material-icons">comments {{comments}}</i>

        </p>
        <p class="stats-item">
            <i class="material-icons">cloud_download</i>
            {{downloads}}
        </p>
    </div>
</div>
{{/each}}